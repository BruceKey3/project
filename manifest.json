{
  "name": "Hello World!",
  "version": "1.0",
  "description": "My first Chrome extension.",
  "manifest_version": 2,
  "permissions": [
    "<all_urls>",
    "webRequest"
  ],
  "browser_action": {
    "default_icon": "icon.png"
    //"default_popup": "popup.html"
  },
  "background": {
    "scripts": ["background.js"],
     "persistent": true
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "all_frames": true,
      "js": ["jquery-3.2.1.js", "content.js"]
    }
  ]
}
